                            return;
                        }
                        CCLink.Ext_SetExtName(LinkExtCode,'<?php echo ($UserName); ?>');
                        //CCLink.Ext_SetExtWorkNo(Com,Com);
                        CCLink.Ext_CheckInQueue(LinkExtCode); //将分机签入队列
                    }
                    return;
                }

                function Link_DisconnectServer() {
                    CCLink.Link_DisConnectServer(); //断开客户端和服务器端连接
                    return;
                }
                //连接CTI服务器
                Link_ConnectServer();
