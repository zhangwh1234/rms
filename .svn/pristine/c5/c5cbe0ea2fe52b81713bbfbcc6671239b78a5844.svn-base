    //启动消息群发系统
    setInterval(function(){
        var url = "__APP__/Messages/getMessages";
        $.ajax({
            type : "GET",
            url : url,
            dataType : "json",
            success : function(data){
                if(data){
                    showMessages(data);
                } 
            }
        });
    },15000);

    function showMessages(msg){
        $.messager.show({
            title:'消息提示',
            msg:msg,
            showType:'show',
            timeout:0

        });
    }
</script>