<script>
/* 关闭选项卡 */
function orderHandleSearchviewSendnamecloseTab(){
    // 返回选项卡
    var tab = $('#operation').tabs('getSelected');
    // 返回选项卡的index
    var index = $('#operation').tabs('getTabIndex',tab);
    // 关闭选项卡
    $('#operation').tabs('close',index);
    //清理一下DOM
    orderHandle.searchviewSendname.table.remove();
    orderHandle.searchviewSendname.beizhuWindow.remove();
    orderHandle.searchviewSendname.beizhuTable.remove();
    orderHandle.searchviewSendname.companyWindow.remove();
    orderHandle.searchviewSendname.companyTable.remove();
    orderHandle.searchviewSendname.sendnameSearchWindow.remove();
    orderHandle.searchviewSendname.sendnameSearchInput.remove();
    orderHandle.searchviewSendname.secondPointWindow.remove();
    orderHandle.searchviewSendname.secondPointTable.remove();

}
</script>